{
  "name": "@andrey-str/aura-ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "tailwind.preset.cjs",
    "tailwind.preset.d.ts",
    "CHANGELOG.md",
    "TOKENS.md",
    "TOKENS_CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": [
    "dist/style.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./tailwind-preset": {
      "types": "./tailwind.preset.d.ts",
      "default": "./tailwind.preset.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist && vite build && node scripts/emit-style.mjs && vue-tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "aura": {
    "tokensVersion": "0.1.0"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@motionone/vue": "^10.16.4"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "echarts": "^5.5.0",
    "vue-echarts": "^7.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/vue3": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "postcss": "^8.5.1",
    "prettier": "3.5.3",
    "storybook": "^10.1.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vue": "^3.5.13",
    "echarts": "^5.5.0",
    "vue-echarts": "^7.0.0",
    "vue-tsc": "^2.2.8"
  }
}
